| 测试当前支持的控制流功能

func main() {
    | 测试for循环（数组遍历）
    a = ["sda", "sds", "sddds"];
    for (i in a) {
        System.print(i);
    }
    
    | 测试break和continue
    counter = 0;
    while counter < 10 {
        counter = counter + 1;
        if counter == 3 {
            continue;  | 跳过3
        }
        if counter == 7 {
            break;     | 在7处停止
        }
        System.print("Counter: " + counter);
    }
    
    | 测试传统for循环
    for (n = 0; n < 5; n = n + 1) {
        System.print("Traditional for: " + n);
    }
}